        "symfony/console": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^7.0"
    },
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Julius HÃ¤rtl",
            "email": "jus@bitgrid.net"
        }
