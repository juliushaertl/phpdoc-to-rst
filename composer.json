{
    "name": "stuba/phpdoc-to-rst",
    "description": "Generate Sphinx/ReStructured documentation from PHPDoc",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stuba/phpdoc-to-rst.git"
        }
    ],
    "require": {
        "phpdocumentor/reflection": "dev-develop#cc98c55",
        "phpdocumentor/reflection-common": "1.0.1@stable",
        "phpdocumentor/reflection-docblock": "4.3.0@stable",
        "symfony/console": "^4.0@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.5@stable"
    },
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Julius HÃ¤rtl",
            "email": "jus@bitgrid.net"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "JuliusHaertl\\PHPDocToRst\\": ["src"],
            "JuliusHaertl\\PHPDocToRst\\Test\\": ["tests"]
        }
    },
    "bin": ["bin/phpdoc-to-rst"]
}